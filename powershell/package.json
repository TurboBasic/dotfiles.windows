{
  "name": ".dotfiles::windows::powershell",
  "version": "0.0.15",
  "description": "Create & maintain full Windows environment. Details: Create Powershell user profile + \n supplementary modules; Create Machine- and User-level environment variables",
  "main": "_install.cmd",
  "files": [
    {
      "name": "logon.ps1"
    },
    {
      "name": "Modules\\Commands.*",
      "destination": "Modules\\Commands"
    },
    {
      "name": "Modules\\Environment.*",
      "destination": "Modules\\Environment"
    },
    {
      "name": "Modules\\Fileutils.*",
      "destination": "Modules\\Fileutils"
    },
    {
      "name": "Microsoft.PowerShell_profile.ps1"
    },
    {
      "name": "Powershell.xml"
    },
    {
      "name": "Set-SolarizedDarkColorDefaults.ps1"
    },
    {
      "name": "Set-SolarizedLightColorDefaults.ps1"
    },
    {
      "name": "package.json"
    },
    {
      "name": "set_environment.cmd"
    },
    {
      "name": "set_environment.ps1"
    }
  ],
  "destination": "$env:UserProfile\\Documents\\WindowsPowerShell"
}
